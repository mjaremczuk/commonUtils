<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:orientation="vertical"
		tools:context="pl.revo.commonhelpers.MainActivity">
	<Button android:id="@+id/example_1"
			android:text="example"
			style="@style/test_style"/>
	<EditText android:id="@+id/edit_text"
			android:visibility="gone"
			android:layout_height="wrap_content"
			android:layout_width="match_parent"/>
	<FrameLayout android:id="@+id/fragment_container"
			android:layout_height="1dp"
			android:layout_width="match_parent"/>
	<FrameLayout android:layout_height="match_parent"
			android:visibility="visible"
			android:layout_width="match_parent">
		<pl.revo.helperutils.WavingView android:id="@+id/waving_view"
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				app:startSize="60dp"
				app:centerImage="@mipmap/ic_launcher"
				app:waveCount="2"
				app:waveDuration="1000"/>
		<pl.revo.helperutils.WavingView android:id="@+id/waving_view2"
				android:layout_height="wrap_content"
				android:layout_width="200dp"
				app:startSize="20dp"
				app:centerImage="@mipmap/ic_launcher"
				app:waveCount="4"
				app:waveDuration="400"/>
	</FrameLayout>
	<!--<RelativeLayout android:layout_width="match_parent"-->
			<!--android:layout_height="match_parent">-->
	<!--<pl.revo.commonhelpers.internal.CustomKeyboard android:layout_width="match_parent"-->
			<!--android:layout_alignParentBottom="true"-->
			<!--android:layout_height="wrap_content"/>-->
	<!--</RelativeLayout>-->
</LinearLayout>
